{
  "expo": {
    "name": "Careconnect React Native Expo",
    "slug": "careconnect-react-native-expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "web": {
      "bundler": "metro",
      "output": "single"
    },
    "newArchEnabled": true,
    "jsEngine": "hermes",
    "scheme": "mobileapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.careconnectreactnativeexpo"
    },
    "plugins": [
      "expo-sqlite"
    ]
  }
}
